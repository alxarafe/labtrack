{
    "name": "alxarafe/labtrack",
    "description": "LabTrack Application",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "repositories": [
        {
            "type": "path",
            "url": "/var/www/alxarafe",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/alxarafe/alxarafe.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "alxarafe/alxarafe": "v0.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "audit": {
            "ignore": [
                "PKSA-y2cr-5h3j-g3ys"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "Modules/",
            "Tests\\": "Tests/"
        }
    }
}